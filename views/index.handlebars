<h1>Cats!</h1>

<h2>Cats that are not sleepy!</h2>

<ul>
  {{#each articles}}
      {{> articles/article-block}}
  {{/each}}
</ul>

<h2>Cats that are sleepy!</h2>

<ul>
  {{#each article}}
      {{title}}
      {{link}}
  {{/each}}
</ul>

<h2>Add a Cat</h2>
<form class="create-form">

  <div class="form-group">
    <label for="ca">Cat Name:</label>
    <input type="text" id="ca" name="name">
  </div>

  <div class="form-group">
    <label for="sleepy">Sleepy?</label><br>
    <input type="radio" name="sleepy" value="1" checked> Sleepy!<br>
    <input type="radio" name="sleepy" value="0"> Awake
  </div>

  <button type="submit">Add Cat</button>
</form>
<body>
    <div id="wrapper">
      <h1>Scrapping Project</h1>
      <h2><i>Demonstrating proficiency with scrapping</i></h2>
      <div id="articles"></div>
      <div id="notes"></div>
      <div id="saved"></div>
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="app.js"></script>
  </body>
